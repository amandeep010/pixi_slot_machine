{
  "name": "pixi-slot-2025",
  "version": "0.0.0",
  "private": true,
  "type": "module",
  "scripts": {
    "start": "npm run dev",
    "build": "npm run lint && tsc && vite build",
    "prettier:list": "prettier \"{src,test}/**/*.{ts,js}\" --list-different",
    "prettier:fix": "prettier \"{src,test}/**/*.{ts,js}\" --write-different",
    "lint": "eslint .",
    "dev": "vite",
    "prepare": "husky"
  },
  "engines": {
    "node": ">=20"
  },
  "lint-staged": {
      "src/*": "prettier --write --ignore-unknown"
  },
  "dependencies": {
    "pixi.js": "^8.8.1"
  },
  "devDependencies": {
    "@eslint/js": "^9.21.0",
    "eslint": "^9.21.0",
    "husky": "^9.1.7",
    "lint-staged": "^15.5.1",
    "prettier": "3.5.3",
    "typescript": "~5.7.3",
    "typescript-eslint": "^8.25.0",
    "vite": "^6.2.0"
  }
}
